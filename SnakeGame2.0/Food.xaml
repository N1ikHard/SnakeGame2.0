<UserControl x:Class="SnakeGame2._0.Food"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SnakeGame2._0"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="20">
    <Canvas x:Name="Contur" Width="20" Height="20">
        <Ellipse Width="20" Height="20" Fill="Black" Panel.ZIndex="0"/>
        <Ellipse Width="18" Height="18" Fill="Orange" Panel.ZIndex="1" Canvas.Top="1" Canvas.Left="1"/>
        <Ellipse Width="14" Height="14" Fill="Black" Panel.ZIndex="2" Canvas.Top="3" Canvas.Left="3"/>
        <Ellipse Width="12" Height="12" Fill="Yellow" Panel.ZIndex="3" Canvas.Top="4" Canvas.Left="4"/>
    </Canvas>
</UserControl>
